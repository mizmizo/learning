#!usr/bin/env roseus

(ros::roseus "homework1022")

(defun 1_1 (num)
  (dotimes (i num)
    (print "."))
  )

(defun 1_2 (num)
  (if (< num 1)
      nil
    (progn (print ".")
	   (1_2 (- num 1)))
    )
  )

(defun 2_1 (argv)
  (setq cnt 0)
  (while (!= nil argv)
    (if (= a (car argv))
	(+ cnt 1)
      nil)
    (setq argv (cdr argv))
    )
  (format t "The number of a : ~A~%" cnt)
  )

(defun 2_2 (argv)
  (setq cnt 0)
  (if (= nil argv)
      (0)
    (if (= a (car argv))
	(+ 1 (2_2 (cdr argv)))
      (2_2 (cdr argv))
      )
    )
  )
